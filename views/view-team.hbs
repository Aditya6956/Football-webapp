<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page">View Team</li>
  </ol>
</nav>

<div class="view-user p-5">
  {{#each rows}}
  <div class="row mb-5">
    <div class="col text-center">
      <h3>{{this.teamName}}</h3>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">League</th>
            <th scope="col">Team Name</th>
            <th scope="col">Team Logo</th>
            <th scope="col">Venue</th>
            <th scope="col">Venue Image</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <th scope="row">{{this.league}}</th>
            <td>{{this.teamName}}</td>
            <td><img src={{this.teamLogo}} alt="No Image" height="300px" width="250px"></td>
            <td>{{this.venue}}</td>
            <td><img src={{this.venueImage}} alt="No Image" height="300px" width="350px"></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  {{/each}}
</div>